<%-include('../layout/header.ejs')%>
<div class="untree_co-section product-section before-footer-section">
	<div class="container">
		  <div class="row">
			<%if(details!=null){%>
				<% details.forEach((product)=>{ %>
					<%if(product.status==true){%>
						<div class="col-12 col-md-4 col-lg-3 mb-5" id="data">
							<a class="product-item" href="#">
								<img src="/public/uploads/<%=product.image%>" class="img-fluid product-thumbnail">
								<h3 class="product-title">Ergonomic Chair</h3>
								<strong class="product-price">$<%=product.price%>.00</strong>
								<span class="icon-cross">
									<img src="/public/user/images/cross.svg" class="img-fluid">
								</span>
							</a>
						</div>	
					<%}%>
				<%})%>
			<%}%>
		  </div>
	</div>
</div>

<%-include('../layout/footer.ejs')%>